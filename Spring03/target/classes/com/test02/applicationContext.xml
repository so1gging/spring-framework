<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean 2개 설정(1.생성자 이용, 2. setter 이용) -->
	
	<!-- 생성자 이용 -->
	<bean id="emp1" class="com.test02.Emp">
		<constructor-arg name="name" value="김솔지"></constructor-arg>
		<constructor-arg name="salary" value="10000"></constructor-arg>
	</bean>
	
	<!-- setter 이용 -->
	<bean id="emp2" class="com.test02.Emp">
		<property name="name" value="최지혜"></property>
		<property name="salary" value="15000"></property>
	</bean>

</beans>
